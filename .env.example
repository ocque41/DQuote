# Supabase
DATABASE_URL="postgresql://postgres:YOUR_PASSWORD@db.YOURHOST.supabase.co:5432/postgres"
DIRECT_URL="postgresql://postgres:YOUR_PASSWORD@db.YOURHOST.supabase.co:5432/postgres" # if using pgbouncer, set a non-pooled DIRECT_URL
SUPABASE_URL="https://YOUR_PROJECT.ref.supabase.co"
SUPABASE_ANON_KEY="supabase-anon-key"
SUPABASE_SERVICE_ROLE_KEY="supabase-service-role-key"
NEXT_PUBLIC_SUPABASE_URL="https://YOUR_PROJECT.ref.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="supabase-anon-key"

# App
NEXT_PUBLIC_APP_URL="http://localhost:3000"
DEMO_PROPOSAL_SHARE_ID="dq-demo-aurora" # optional override for the admin analytics dashboard
# Optional: configure where receipt PDFs are written when deploying to object storage
PDF_STORAGE_BUCKET="public/receipts"

# Stripe
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="pk_test_..."

# Puppeteer (optional - set if deploying with a custom Chromium binary)
PUPPETEER_EXECUTABLE_PATH=""

# Email SMTP (for acceptance receipts)
SMTP_HOST="smtp.example.com"
SMTP_PORT="587"
SMTP_USER="apikey"
SMTP_PASSWORD="your-smtp-password"
EMAIL_FROM="DQuote <noreply@yourdomain.com>"

# Security
ENCRYPTION_KEY="generate_a_32_char_key"
